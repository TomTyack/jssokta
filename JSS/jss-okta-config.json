{
  "mock": {
    "islocal": false,
    "mockApiEndpoint": "https://okta-apis.azurewebsites.net"
  },
  "okta": {
    "baseDomain": "https://dev-INSERT-OKTA-ID.okta.com",
    "issuer": "https://dev-INSERT-OKTA-ID.okta.com/oauth2/default",
    "redirectUri": "https://jssokta/implicitcallback",
    "clientId": "${YOUR_APPLICATION_CLIENT_ID}",
    "pkce": true,
    "scopes": ["openid", "profile", "email"],
    "portalRoute": "/profile",
    "loginRoute": "/login",
    "logoutRoute": "/logout"
  },
  "debug": {
    "showApiErrors": false,
    "showDebugJson": false
  }
}
